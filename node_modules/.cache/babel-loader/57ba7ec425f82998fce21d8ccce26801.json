AIA,IAAI,CAAChB,EAAL,KAAYA,EAAhB;AAAA,KAAvB,CAA7B;AAAA,GAHE;AAIdW,EAAAA,OAAO,EAAE,iBAAChC,KAAD,EAAQgC,QAAR,EAAoB;AACzB,QAAMS,GAAG,GAAGzC,KAAK,CAACC,KAAN,CAAYyC,SAAZ,CAAsB,UAAAL,IAAI;AAAA,aAAIA,IAAI,CAAChB,EAAL,KAAYW,QAAO,CAACX,EAAxB;AAAA,KAA1B,CAAZ;;AACA,QAAIoB,GAAG,KAAK,CAAC,CAAb,EAAgB;AACZzC,MAAAA,KAAK,CAAC2C,MAAN,CAAaF,GAAb,EAAkB,CAAlB,EAAqBT,QAArB;AACH;AACJ;AATa,CAAlB;AAcA,eAAe;AACXhC,EAAAA,KAAK,EAALA,KADW;AAEXE,EAAAA,OAAO,EAAPA,OAFW;AAGXE,EAAAA,OAAO,EAAPA,OAHW;AAIX8B,EAAAA,SAAS,EAATA;AAJW,CAAf","sourcesContent":["import axios from 'axios';\r\n\r\nconst state = {\r\n    todos:\r\n        []\r\n\r\n};\r\n\r\nconst getters = {\r\n    allTodos: (state) => state.todos,\r\n};\r\n\r\nconst actions = {\r\n    async fetchTodos({ commit }) {\r\n\r\n        const response = await axios.get(process.env.VUE_APP_JP_BASE_API_URL);\r\n        window.console.log(response);\r\n        window.console.log(commit);\r\n        commit('setTodos', response.data);\r\n    },\r\n    async addTodo({ commit }, title) {\r\n\r\n        const response = await axios.post(process.env.VUE_APP_JP_BASE_API_URL,\r\n            { title, completed: false });\r\n        window.console.log(response);\r\n        window.console.log(commit);\r\n        commit('newTodo', response.data);\r\n    },\r\n    async deleteTodo({ commit }, id) {\r\n        const response = await axios.delete(`${process.env.VUE_APP_JP_BASE_API_URL}/${id}`);\r\n        window.console.log(response);\r\n        window.console.log(commit);\r\n        commit('removeTodo', id);\r\n    },\r\n    async filterTodos({ commit }, e) {\r\n        //get selected number\r\n        const limit = parseInt(e.target.options[e.target.options.selectedIndex].innerText);\r\n        const response = await axios.get(`${process.env.VUE_APP_JP_BASE_API_URL}/?_limit=${limit}`);\r\n        window.console.log(response);\r\n        window.console.log(commit);\r\n        commit('setTodos', response.data);\r\n    },\r\n    async updateTodo({ commit }, updTodo) {\r\n        const response = await axios.put(`${process.env.VUE_APP_JP_BASE_API_URL}/${updTodo.id}`, updTodo);\r\n        window.console.log(response);\r\n        window.console.log(commit);\r\n        commit('updTodo', response.data);\r\n    },\r\n};\r\n\r\nconst mutations = {\r\n    setTodos: (state, todos) => (state.todos = todos),\r\n    newTodo: (state, todo) => (state.todos.unshift(todo)),\r\n    removeTodo: (state, id) => state.todos = state.todos.filter(todo => todo.id !== id),\r\n    updTodo: (state, updTodo) => {\r\n        const idx = state.todos.findIndex(todo => todo.id === updTodo.id);\r\n        if (idx !== -1) {\r\n            state.splice(idx, 1, updTodo);\r\n        }\r\n    }\r\n};\r\n\r\n\r\n\r\nexport default {\r\n    state,\r\n    getters,\r\n    actions,\r\n    mutations\r\n}"]}]}